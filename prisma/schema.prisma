generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int     @id @default(autoincrement())
  email      String? @db.VarChar(64)
  first_name String? @db.VarChar(64)
  last_name  String? @db.VarChar(64)
  password   String? @db.VarChar(256)
}
